                    "meta-openembedded/meta-oe",
                    "meta-raspberrypi"
                ],
                "bb-layers-file-relative": [
                    "meta-application"
                ],
				"oe-fragments": ["core/yocto/sstate-mirror-cdn", "machine/raspberrypi-armv8", "application/wifi-ex"],
                "oe-fragments-one-of": {
                    "distro": {
                        "description": "Distribution variants",
                        "options": ["distro/poky-altcfg"]
                    }
                }
            }
        ]
